{
  "name": "amiiiiink/hello-world",
  "description": "A simple Hello World Laravel package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amin Karimi",
      "email": "karimi66.amin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Amiiiiink\\HelloWorld\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Amiiiiink\\HelloWorld\\HelloWorldServiceProvider"
      ],
      "aliases": {
        "HelloWorld": "Amiiiiink\\HelloWorld\\Facades\\HelloWorld"
      }
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": "^9|^10|^11"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
